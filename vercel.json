{
  "version": 2,
  "name": "koquifi-lottery-frame",
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index.js"
    },
    {
      "source": "/.well-known/(.*)",
      "destination": "/api/index.js"
    }
  ]
}